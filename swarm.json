{
  "title": "Agency Swarm Setup",
  "description": "Script to clone the Agency Swarm repository and set up the environment.",
  "icon": "https://raw.githubusercontent.com/VRSEN/agency-swarm/main/logo.png", // Add icon URL here
  "run": [
    {
      "method": "shell.start"
    },
    {
      "method": "shell.enter",
      "params": {
        "message": "git clone https://github.com/VRSEN/agency-swarm.git && cd agency-swarm"
      }
    },
    {
      "method": "shell.enter",
      "params": {
        "message": "pip install -r requirements.txt"
      }
    },
    {
      "method": "shell.enter",
      "params": {
        "message": "python setup.py install"
      }
    }
  ]
}
